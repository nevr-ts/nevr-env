{
  "name": "@nevr-env/cli",
  "version": "0.1.0",
  "description": "Interactive CLI for nevr-env with the Wizard fixer",
  "type": "module",
  "bin": {
    "nevr-env": "./dist/index.js",
    "nevr": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@clack/prompts": "^0.9.0",
    "@nevr-env/core": "workspace:*",
    "c12": "^2.0.0",
    "chalk": "^5.4.0",
    "commander": "^13.0.0",
    "dotenv": "^16.5.0",
    "jiti": "^2.4.0",
    "picocolors": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18",
    "zod": "^3.24.0"
  },
  "keywords": [
    "nevr-env",
    "cli",
    "env",
    "environment",
    "wizard"
  ],
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "author": "Yalelet Dessalegn",
  "homepage": "https://nevr-ts.github.io/nevr-env/",
  "bugs": {
    "url": "https://github.com/nevr-ts/nevr-env/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nevr-ts/nevr-env",
    "directory": "packages/cli"
  }
}
